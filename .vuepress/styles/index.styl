.contact-item
  display: inline-block
  padding: 5px

.iframe-container {
  position: relative;
  overflow: hidden;
  padding-top: 80%;
  border: 1px solid rgba(0,0,0, .125);
  border-radius: 6px;
}

.iframe-container iframe {
    position: absolute;
    top: 0;
    border: 0;
    margin: 0;
    height: 100%;
    width: 100%;
}
